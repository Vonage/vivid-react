  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@octokit/core": "^3.1.2",
    "@vonage/vwc-button": "0.13.0",
    "@vonage/vwc-icon": "0.13.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "extract-zip": "2.0.1",
    "mkdirp": "1.0.4",
    "web-component-analyzer": "^1.1.6"
